#
# Automatically generated make config: don't edit
#
CONFIG_PRJ_NAME="Tempalte"

#
# Device Drivers
#
# CONFIG_DRV_GPIO is not set
CONFIG_DRV_UART=y
CONFIG_DRV_UART1=y
CONFIG_DRV_UART1_BAUDRATE=115200
CONFIG_DRV_UART1_PARITY="USART_Parity_No"
CONFIG_DRV_UART1_WORDLENGTH="USART_WordLength_8b"
CONFIG_DRV_UART1_STOPBITS="USART_StopBits_1"

#
# Uart Pio config
#
CONFIG_DRV_UART1_GPIO="GPIOA"
CONFIG_DRV_UART1_RXPIO="GPIO_Pin_9"
CONFIG_DRV_UART1_TXPIO="GPIO_Pin_10"
# CONFIG_DRV_UART2 is not set

#
# System
#
CONFIG_BOARD_CUSTOM=y
# CONFIG_LCD_STM32 is not set
# CONFIG_SIMU_LINUX is not set

#
# Choose arch
#
CONFIG_ARCH_STM32=y
# CONFIG_ARCH_LINUX is not set

#
# choose CPU
#
# CONFIG_CPU_STM32F102 is not set
# CONFIG_CPU_STM32F103RBT6 is not set
CONFIG_CPU_STM32F207ZGT6=y
# CONFIG_CPU_STM32F103ZET6 is not set

#
# sub arch
#
# CONFIG_SUB_STM32F1XX is not set
CONFIG_SUB_STM32F2XX=y

#
# memory
#
# CONFIG_EXTERNAL_RAM is not set
# CONFIG_EXTERNAL_ROM is not set

#
# arch lib
#
CONFIG_LIB_SOURCE_ARCHIVE_NAME_F2="stm32f2xx_stdperiph_lib.zip"
CONFIG_LIB_SOURCE_PATH="http://www.st.com/internet/com/SOFTWARE_RESOURCES/SW_COMPONENT/FIRMWARE/"
